{
    "ConnectionStrings": {
        "ConfigurationDbConnection": "Server=localhost;port=3306;DataBase=projectAuth;Uid=root;Pwd=",
        "PersistedGrantDbConnection": "Server=localhost;port=3306;DataBase=projectAuth;Uid=root;Pwd=",
        "IdentityDbConnection": "Server=localhost;port=3306;DataBase=projectAuth;Uid=root;Pwd=",
        "AdminLogDbConnection": "Server=localhost;port=3306;DataBase=projectAuth;Uid=root;Pwd="
    },
  "AdminConfiguration": {
    "IdentityAdminBaseUrl": "http://localhost:9000",
    "IdentityAdminRedirectUri": "http://localhost:9000/signin-oidc",
    "IdentityServerBaseUrl": "http://localhost:5000",
    "ClientId": "AdminManager",
    "ClientSecret": "adminClientSecret",
    "OidcResponseType": "code id_token",
    "Scopes": [
      "openid",
      "profile",
      "email",
      "roles"
    ]
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Error",
      "Override": {
        "Skoruba": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Log\\skoruba_admin.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "Server=localhost;port=3306;DataBase=projectAuth;Uid=root;Pwd=",
          "tableName": "Log",
          "columnOptionsSection": {
            "addStandardColumns": [ "LogEvent" ],
            "removeStandardColumns": [ "Properties" ]
          }
        }
      }
    ]
  }
}